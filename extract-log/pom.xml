  <groupId>com.aladdin</groupId>
  <artifactId>extract-log</artifactId>
  <version>0.1-SNAPSHOT</version>
  <packaging>jar</packaging>

  <name>extract-log</name>
  <!-- FIXME change it to the project's website -->
