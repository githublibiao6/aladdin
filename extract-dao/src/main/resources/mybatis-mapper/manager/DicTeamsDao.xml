<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aladdin.mis.dao.manager.DicTeamsDao">

    <resultMap id="DictObjResultMap" type="com.aladdin.mis.chat.manager.bean.DictionaryTeams">

    </resultMap>

    <!-- 根据id获取员工信息 -->
    <select id="listTeamsByDicCode" resultType="com.aladdin.mis.chat.manager.bean.DictionaryTeams">
        select t.* from be_dictionary_teams t
        left join be_dictionary m  on t.dic_id = m.id
        where m.code = #{code} and t.sys005=1
    </select>
</mapper>
